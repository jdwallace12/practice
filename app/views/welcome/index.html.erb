<div id="skrollr-body">
  <div class="row">
    <div class="heading_input col-md-12">
      <input type="text" placeholder="Type Something Here" ng-model="heading"/>
      <input type="text" placeholder="And Here" ng-model="caption"/>
      <input type="text" placeholder="Type Something Here" ng-model="heading2"/>
      <input type="text" placeholder="And Here" ng-model="caption2"/>
    </div>
    <div class="hero-scroll col-md-12" data-0="background-color:rgb(0,0,255);" data-center="background-color:rgb(255,0,0);">
      <div class="hero-heading" data-0="transform:rotate(0deg); position:absolute; right:0%; top:0%; opacity:.6; font-weight:100;" data-top="transform:rotate(360deg);  position:absolute; right:50%; top:70%; opacity:1;font-weight:800;">
        <h1 data-0="font-weight:800;" data-top="font-weight:100;">{{heading}}</h1>
      </div>
      <div class="hero-subheading" data-0="transform:rotate(0deg); position:absolute; left:0%; bottom:0%;opacity:1;" data-top="transform:rotate(180deg);  position:absolute; left:50%; bottom:90%;opacity:.6;">
        <p data-0="font-weight:100;" data-15="font-weight:300;" data-30="font-weight:800;">{{caption}}</p>
        </p>
      </div>
      <div class="hero-heading" data-0="transform:rotate(0deg); position:absolute; left:0%; top:30%; opacity:.6;" data-top="transform:rotate(360deg);  position:absolute; right:50%; lef top:70%; opacity:1;">
        <h1 data-0="font-weight:800;" data-top="font-weight:100;">{{heading2}}</h1>
      </div>
      <div class="hero-subheading" data-0="transform:rotate(0deg); position:absolute; right:0%; bottom:0%;opacity:1;" data-top="transform:rotate(180deg);  position:absolute; right:50%; bottom:90%;opacity:.6;">
        <p data-0="font-weight:100;" data-15="font-weight:300;" data-30="font-weight:800;">{{caption2}}</p>
        </p>
      </div>
    </div>
  </div>
